                    <font>
                        <Font name="Lucida Fax Regular" size="12.0" />
                    </font>
                </Button>
                <Button layoutX="295.0" layoutY="385.0" mnemonicParsing="false" onAction="#openDoodle" prefHeight="58.0" prefWidth="153.0" style="-fx-background-color: black; -fx-border-radius: 10; -fx-background-radius: 15; -fx-text-fill: white; -fx-font-weight: bold; -fx-font-size: 16px; -fx-font-family: 'Monospace';" text="Doodle">
                    <font>
                        <Font name="Lucida Fax Regular" size="12.0" />
                    </font>
                </Button>
            </children>
        </Pane>
    </children>
</AnchorPane>
