<?import javafx.scene.text.*?>

<AnchorPane prefHeight="650.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.imageeditor.HomeController">
    <children>
        <ImageView fitHeight="650.0" fitWidth="1300.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@imgs/turq.jpg" />
            </image>
        </ImageView>

        <Text layoutX="395.0" layoutY="260.0" text="SnapEdit">
            <font>
                <Font name="Rage Italic" size="125.0" />
            </font>
        </Text>

        <ImageView fitHeight="83.0" fitWidth="80.0" layoutX="865.0" layoutY="220.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@imgs/camera.png" />
            </image>
        </ImageView>

        <Pane layoutX="258.0" layoutY="61.0" prefHeight="476.0" prefWidth="743.0">
